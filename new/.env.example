# MediScan AI Backend Configuration
# Copy this file to .env and update values as needed

# JWT Secret Key - KEEP THIS SECRET AND CONSISTENT!
# This ensures tokens remain valid across server restarts
JWT_SECRET_KEY=mediscan-ai-super-secret-key-2026-do-not-share

# Token expiration in hours
TOKEN_EXPIRATION_HOURS=24

# Gemini API Key for AI prescriptions
GEMINI_API_KEY=your-gemini-api-key-here

# Flask Configuration
FLASK_DEBUG=false
FLASK_PORT=5000

# TensorFlow Configuration - Suppress verbose logs
TF_CPP_MIN_LOG_LEVEL=2
TF_ENABLE_ONEDNN_OPTS=0 
